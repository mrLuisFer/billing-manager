{
  "extends": [
    "airbnb",
    "airbnb-typescript",
    "next",
    "next/core-web-vitals",
    "prettier"
  ],
  "parserOptions": {
    "project": "./tsconfig.json"
  },
  "ignorePatterns": ["**/*.js", "**/*.jsx"],
  "plugins": ["prettier"],
  "rules": {
    "react/jsx-props-no-spreading": "off",
    "react/require-default-props": "off",
    "prettier/prettier": "warn",
    "no-console": "warn"
  }
}
